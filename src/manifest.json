{
    "name": "Beijing-guahao",
    "description": "Beijing guahao",
    "version": "0.1.0",
    "background": {
        "scripts": ["/js/background.js"]
    },
    "page_action": {
        "default_icon": "/images/icon_128.png",
        "default_title": "Gua Hao",
        "default_popup": "/html/pop.html"
    },
    "content_scripts": [
        {
            "matches": ["http://www.bjguahao.gov.cn/*"],
            "js": ["/js/jquery-2.0.3.min.js","/js/mycontent.js"]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "/images/icon_16.png",
        "128": "/images/icon_128.png"
    },
    "manifest_version": 2
}